{
    "name": "@rossipedia/thumb-tack",
    "description": "Chrome Extension for Automatically Pinning Tabs",
    "version": "0.0.5",
    "license": "MIT",
    "scripts": {
        "dev": "nodemon -w src/manifest.json -w webpack.config.js -x \"webpack --mode development --watch\"",
        "build": "rimraf dist && cross-env NODE_ENV=production webpack --mode production",
        "clean": "git clean -xdf -e /.env",
        "upload": "node dotenv.js webstore upload --source dist",
        "version": "node patch-manifest.js"
    },
    "devDependencies": {
        "@types/chrome": "^0.0.69",
        "@types/react": "^16.4.2",
        "@types/react-dom": "^16.0.6",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.4",
        "babel-plugin-styled-components": "^1.5.1",
        "chrome-webstore-upload": "^0.2.2",
        "chrome-webstore-upload-cli": "^1.1.1",
        "copy-webpack-plugin": "^4.5.2",
        "cross-env": "^5.2.0",
        "dotenv": "^6.0.0",
        "html-webpack-include-assets-plugin": "^1.0.4",
        "html-webpack-plugin": "^3.2.0",
        "immer": "^1.3.1",
        "nodemon": "^1.17.5",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "rimraf": "^2.6.2",
        "semver": "^5.5.0",
        "styled-components": "^3.3.3",
        "ts-loader": "^4.4.1",
        "typescript": "^2.9.2",
        "uglifyjs-webpack-plugin": "^1.2.7",
        "webpack": "^4.12.1",
        "webpack-cli": "^3.0.8",
        "webpack-serve": "^1.0.4"
    }
}
